  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/preset-env": "7.23.5",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.38.0",
    "eslint-config-htmlacademy": "9.0.0",
    "gh-pages": "^6.2.0",
    "html-webpack-plugin": "5.5.4",
    "style-loader": "3.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "dayjs": "1.11.10",
    "flatpickr": "4.6.13",
    "he": "1.2.0"
